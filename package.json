{
  "version": "0.1.0",
  "description": "AI Tools SDK",
  "keywords": [
    "Samagra",
    "AI Tools",
    "Speech to text",
    "Text to speech",
    "Speech to number",
    "translate"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "ai-tools-sdk",
  "repository": {
    "type": "git",
    "url": "https://github.com/AgrI-Mitra/AIToolsSDK"
  },
  "author": "Prateek Jakhar",
  "module": "dist/ai-tools-sdk.esm.js",
  "size-limit": [
    {
      "path": "dist/ai-tools-sdk.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/ai-tools-sdk.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^8.2.6",
    "husky": "^8.0.3",
    "size-limit": "^8.2.6",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.5"
  }
}
